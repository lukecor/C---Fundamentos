using System;
using System.Collections;
using System.Collections.Generic;
					
public class Queue
{
	
	public static void Main()
	{
		var Fila = new Queue<string>();
		
		Fila.Enqueue("Fulano");      // Enqueue means add items on the  generic Queue;
		Fila.Enqueue("Sicrano");
		Fila.Enqueue("Beltrano");
		
		Console.WriteLine(Fila.Peek()); // Peek means that the console will print the first stored variable;
		Console.WriteLine(Fila.Count); // and count will be used for count the number of the variables from the list;
		
		Console.WriteLine(fila.Dequeue()); // Dequeue is the reverse of queue, he will "exclude" the first name named on the queue;
		Console.WriteLine(fila.Count);
		
		foreach(var pessoa in Fila) 
		{
		Console.WriteLine(pessoa);
		
		}
		var Salada = new Queue();
		salada.Enqueue(3);
		salada.Enqueue("Item");
		salada.Enqueue(true); // the same situations like above, but here the class isn't generic;
		salada.Enqueue(3.14);
		
		Console.WriteLine(salada.Contains("Item")); // and Contains means "if on the salada variable has the string item";
		


		
	}
}
