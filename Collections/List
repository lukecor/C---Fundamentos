using System;
using System.Collections.Generic;


public class Produto 
{
	public string Nome;
	public double Preco;
	
	public Produto(string nome, double preco)
	{
		Nome = nome;
		Preco = preco;
	}
}

class ColecoesList 
{
	
	public static void Executar() 
	{
	var livro = new Produto("Skyrim ", 234.50);
		
	var carrinho = new List<Produto>();     // this will use the System.Collections above, class list type produto;
	// <> means generic;
	carrinho.Add(livro);
		
	var combo = new List<Produto>
	{
		new Produto("Camisa", 29.90),
		new Produto("8Âª Temporada Game of Thrones", 99.0),
		new Produto("Poster", 10)
	};
		carrinho.AddRange(combo); {
		Console.WriteLine(carrinho.Count);
		carrinho.RemoveAt(3);
	}
	foreach(var item in carrinho)
	{
	Console.WriteLine(carrinho.IndexOff(item));
	Console.WriteLine($"{item.Nome} {item.Preco}");
	};
  }
}
