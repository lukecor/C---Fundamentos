using System;
using System.Collections;


	class ColecoesArrayList
	{
		public static void Executar() 
		{
			var arraylist = new ArrayList {
				"Palavra",
				3,
				true // on array list you can stor many differents typed variables;
			};
			
			arraylist.Add(3.14);
			
			foreach (var item in arraylist) 
			{
				Console.WriteLine("{0} => {1}", item, item.GetType()); // item.GetType means to show what is the type of the variable used above;
			}
			
		
		}
	
	
	}
