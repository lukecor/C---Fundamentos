using System;
using System.Collections.Generic;
using System.Text;

	class ParametroPardrao
	{
		public static int Somar(int a = 1, int b = 1) // Even if you pass the parameteer on an method, you can change on you console...;
		{
			return a + b;
		
		}
		public static void Executar()
		{
			Console.WriteLine(Somar(10, 23)); // ... simply don't returning the parameter named on the metho;
			Console.WriteLine(Somar(50));
			Console.WriteLine(Somar());

		}
	
	}
